"
" ~/.vimrc
"

set nocompatible

filetype indent on
filetype plugin indent on
set autoindent
set cursorline
set expandtab
set ignorecase
set incsearch
set laststatus=2
set lazyredraw
set nobackup
set nohlsearch
set nowrap
set nowritebackup
set number
set ruler
set shiftround
set shiftwidth=4
set showcmd
set showmatch
set smarttab
set softtabstop=4
set t_Co=256
set tabstop=8
set wildmenu
syntax enable
